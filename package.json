{
  "name": "ssr",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node lib/server.js",
    "build": "NODE_ENV=production run-s build:*",
    "build:client": "webpack",
    "build:server": "babel -d lib src",
    "watch": "run-s build:client; run-p watch:*",
    "watch:client": "webpack -w",
    "watch:server": "nodemon --exec babel-node src/server.js",
    "watch:src": "babel -d lib src -w"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "express": "^4.15.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "webpack": "^3.0.0"
  }
}
